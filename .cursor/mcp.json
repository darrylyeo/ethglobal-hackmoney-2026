{
  "mcpServers": {
    "svelte": {
      "command": "deno",
      "args": [
        "run",
        "-A",
        "npm:@sveltejs/mcp"
      ]
    },
    "voltaire": {
      "command": "deno",
      "args": [
        "run",
        "-A",
        "npm:mcp-remote",
        "https://voltaire.tevm.sh/mcp"
      ]
    }
  }
}
